export WZSH_HOME=${HOME}/.zsh
# 加载基础命令
source ${WZSH_HOME}/lib/basic.zsh
# echo 'source ~/.zshenv'
zinfo '开始加载命令'
zinfo '加载 ~/.zshenv'
